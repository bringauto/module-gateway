{
  "general-settings": {
    "log-path": "./",
    "verbose": true
  },
  "internal-server-settings": {
    "port": 8888
  },
  "module-paths": {
    "1": "./libmission-module-gateway-sharedd.so",
    "2": "./libio-module-gateway-shared.so",
    "1000": "./libmission-module-gateway-shared.so"

  },
  "external-connection" : {
    "company" : "bringauto",
    "vehicle-name" : "virtual_vehicle",
    "endpoints" :
    [
      {
        "protocol-type" : "mqtt",
        "mqtt-settings" : {
          "ssl" : false,
          "ca-file" : "ca.pem",
          "client-cert" : "client.pem",
          "client-key" : "key.pem"
        },
        "server-ip": "localhost",
        "port": 1883,
        "modules": [ 1, 2, 1000 ]
      }
    ]
  }

}
